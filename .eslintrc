{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 2021
  },
  "extends": [
      "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "indent": ["error", "tab", { "SwitchCase": 1 }],
    "semi": [2, "always"],
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/no-empty-interface": "off"
  }
}
